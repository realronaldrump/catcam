[
  {
    "id": "deploy",
    "execute-command": "/var/scripts/deploy.sh",
    "command-working-directory": "/app",
    "trigger-rule": {
      "match": {
        "type": "payload-hash-sha1",
        "secret": "{{WEBHOOK_SECRET}}",
        "parameter": {
          "source": "header",
          "name": "X-Hub-Signature"
        }
      }
    }
  }
]